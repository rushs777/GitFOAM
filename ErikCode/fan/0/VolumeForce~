FoamFile
{
    version     3.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      VolumeForce;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -2 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    inlet
    {
        type            pressureInletVelocity;
        phi             phi;
        value           uniform ( 0 0 0 );
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform ( 0 0 0 );
        value           uniform ( 0 0 0 );
    }
    walls
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
/*    fan_half0
    {
        type            cyclic;
    }
    fan_half1
    {
        type            cyclic;
    }*/
}


