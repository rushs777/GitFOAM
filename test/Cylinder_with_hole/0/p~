FoamFile
{
    version     3.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    down
    {
 	type 		zeroGradient;
    }
    right
    {
 	type 		fixedValue;
	value 		uniform 0;
    }
    up
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
}

