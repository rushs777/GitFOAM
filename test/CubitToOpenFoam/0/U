FoamFile
{
    version     3.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    topBottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
	type		zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (3 0 0);
    }
}

