FoamFile
{
    version     3.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    down
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    right
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    movingTop
    {
        type            fixedValue;
        value           uniform (5 0 0);
    }
    left
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

